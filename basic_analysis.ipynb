   "source": [
    "nDrafts = draftCount\n",
    "prob = pairs*1           # Make a copy, and also make symmetric\n",
    "\n",
    "for i in range(nCards):\n",
    "    for j in range(i+1):\n",
    "        if freq[i]*freq[j]>0:\n",
    "            prob[i,j] = prob[i,j]/(freq[i]*freq[j])*nDrafts\n",
    "            prob[j,i] = prob[i,j]\n",
    "        \n",
    "dist = (1-0.99*prob/prob.max())\n",
    "plt.imshow(dist, cmap=\"Greys\");"
